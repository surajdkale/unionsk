<h3>Program 3RD -VEHICLES TYPES</h3>
<hr>
<?php

  class vehicle
  {
   public $BMW;
   public $INNOVA;
   public $THAR;
   public $NANO;
   // For BMW
   function bmw($BMW)
   {
    $this->bmw=$BMW;
    }
   function get_bmw()
   {
    return $this->bmw;
    }
    //FOR INNVOA
    function innova($INNOVA)
    {
     $this->innova=$INNOVA;
     }
    function get_innova()
    {
     return $this->innova;
     }
    //FOR THAR
    function thar($THAR)
    {
     $this->thar=$THAR;
     }
    function get_thar()
    {
     return $this->thar;
     }
    //FOR NANO
    function nano($NANO)
    {
     $this->nano=$NANO;
     }
    function get_nano()
    {
    return $this->nano;
    }
   }
    
    //assigning values..
    //For BMW
    $BMW=new vehicle();
    $BMW->bmw('BMW');
    //FOr innova
    $INNOVA=new vehicle();
    $INNOVA->innova('INNOVA');
    //FOR thar
    $THAR=new vehicle();
    $THAR->thar('THAR');
    //FOR Nano
    $NANO=new vehicle();
    $NANO->nano('NANO');
    
    //Printing the output...
    echo $BMW->get_bmw()." Is The Speed Car";
    echo "<br>";
    echo $INNOVA->get_innova()."Is The Family-Travel Car";
    echo "<br>";
    echo $THAR->get_thar()."Is The Offroad+Onroad Car";
    echo "<br>";
    echo $NANO->get_nano()."Is The Entry-Level Car";
    
    
    ?>
