  <h3>Program 4TH -BASE_WATTS</h3>
  <hr>
  <?php
  
    class hometheater
    {
     public $XAOMI;
     public $REALME;
     public $SONY;
     public $ZEBRONIC;
     //For xaomi
     function xaomi($XAOMI)
     {
       $this->xaomi=$XAOMI;
      }
      function get_xaomi()
      {
       return $this->xaomi;
       }
      //For Realme
      function realme($REALME)
      {
       $this->realme=$REALME;
       }
       function get_realme()
       {
        return $this->realme;
        }
       //For SONY
       function sony($SONY)
       {
         $this->sony=$SONY;
         }
        function get_sony()
        {
         return $this->sony;
         }
         //FOR Zebronic
        function zebronic($ZEBRONIC)
        {
         $this->zebronic=$ZEBRONIC;
         }
        function get_zebronic()
        {
         return $this->zebronic;
        }
      } 
         //assigning values
       $XAOMI= new hometheater();
       $XAOMI->xaomi('70-Watts');
       $REALME= new hometheater();
       $REALME->realme('100-Watts');
       $SONY=new hometheater();
       $SONY->sony('75-watts');
       $ZEBRONIC=new hometheater();
       $ZEBRONIC->zebronic('65-watts');
       
       //Printing ...
       echo "This is Xaomi its have : ".$XAOMI->get_xaomi();
       echo "<br>";
       echo "This is Realme its have : ".$REALME->get_realme();
       echo "<br>";
       echo "This is Sony its have : ".$SONY->get_sony();
       echo "<br>";
       echo "This is Zebronic its have : ".$ZEBRONIC->get_zebronic();
         ?>
         <hr>
         <h3>Program 5TH -MOBILE PRIZES</h3>
         <hr>
         <?php
         class mobiles
    {
     public $XAOMI;
     public $REALME;
     public $SONY;
     public $ONEPLUS;
     //For xaomi
     function xaomi($XAOMI)
     {
       $this->xaomi=$XAOMI;
      }
      function get_xaomi()
      {
       return $this->xaomi;
       }
      //For Realme
      function realme($REALME)
      {
       $this->realme=$REALME;
       }
       function get_realme()
       {
        return $this->realme;
        }
       //For SONY
       function sony($SONY)
       {
         $this->sony=$SONY;
         }
        function get_sony()
        {
         return $this->sony;
         }
         //FOR OnePlus
        function oneplus($ONEPLUS)
        {
         $this->oneplus=$ONEPLUS;
         }
        function get_oneplus()
        {
         return $this->oneplus;
        }
      } 
         //assigning values
       $XAOMI= new mobiles();
       $XAOMI->xaomi('above 5000');
       $REALME= new mobiles();
       $REALME->realme('Above 6000');
       $SONY=new mobiles();
       $SONY->sony('Above 35,000');
       $ONEPLUS=new mobiles();
       $ONEPLUS->oneplus('Above 30,000');
       
       //Printing ...
       echo "This is Xaomi its have Intial Price : ".$XAOMI->get_xaomi();
       echo "<br>";
       echo "This is Realme its have Intial Price : ".$REALME->get_realme();
       echo "<br>";
       echo "This is Sony its have Intial Price : ".$SONY->get_sony();
       echo "<br>";
       echo "This is Oneplus its have Intial Price : ".$ONEPLUS->get_oneplus();
         ?>
