    <h3>Program 1ST -DESKTOP</h3>
    <hr>
        <?php
        
        // here we creating a class to modify some essentioal data...
        class desktop
        {
            public $RAM;
            public $ROM;
            public $Graphic_Card;
            public $Processor;
            
        
        // Now we create a function which stores the values of classes into it..
        function RAM($RAM)
        {
            $this->ram=$RAM;
        }
               function get_RAM()
        {
            return $this->ram;
        }
        function ROM($ROM)
        {
            $this->rom=$ROM;
        }
            function get_ROM()
        {
            return $this->rom;
        }
        function Graphic_card($Graphic_card)
        {
            $this->graphic_card=$Graphic_card;
        }
            function get_Graphic_card()
        {
            return $this->graphic_card;
        }
        function Processor($Processor)
        {
            $this->processor=$Processor;
        }
               function get_Processor()
        {
            return $this->processor;
        }
 
        }
        

        $RAM= new desktop();
        $ROM= new desktop();
        $Graphic_card= new desktop();
        $Processor= new desktop();
        // Now we add the values..
        $RAM->RAM('4 GB LPDDR4');
        $ROM->ROM('1TB HDD');
        $Graphic_card->Graphic_card('NVIDA 1080TI');
        $Processor->Processor('AMD RYZEN 3 3200U');
        //Now we simply use echo() to print the values...
        echo " DESKTOP FEATURES:<br>";
        echo "RAM : ".$RAM->get_RAM();
        echo "<br>";
        echo "ROM : ".$ROM->get_ROM();
        echo "<br>";
        echo "Graphic Card : ". $Graphic_card->get_Graphic_card();
        echo "<br>";
        echo "Processor : ".$Processor->get_processor();
        
        ?>
        <hr>
        <h3>Program 2ND -FAMILY</h3>
        <hr>
        <?php
        //here we now take a family names of each person and its designations....
class family_members
{
  public $Me;
  public $Father;
  public $Mother;
  public $Sister;
 // Now we simply creating a functions 
 function Me($ME)
 {
   $this->me=$ME;
 }
  function get_Me()
 {
  return $this->me;
 }
 function Father($FATHER)
  {
   $this->father=$FATHER;
  }
  function get_Father()
 {
  return $this->father;
  }
 function Mother($MOTHER)
 {
  $this->mother=$MOTHER;
 }
  function get_Mother()
 {
  return $this->mother;
 }
 function Sister($SISTER)
 {
  $this->sister=$SISTER;
 }
  function get_Sister()
 {
  return $this->sister;
 }

}
 // Now we creating some variables ..
 $ME= new family_members();
 $FATHER= new family_members();
 $MOTHER= new family_members();
 $SISTER= new family_members();
 
 // Now we declaring the values in it...
 $ME->Me('Rohit');
 $FATHER->Father('Vinay');
 $MOTHER->Mother('Reshma');
 $SISTER->Sister('Rutuja');
 
 //Now we printing the output....
 
 echo "My name is ".$ME->get_Me();
 echo "<br>";
 echo "My Father name is ".$FATHER->get_Father();
 echo "<br>";
 echo "My Mother name is ".$MOTHER->get_Mother();
 echo "<br>";
 echo "My Sister name is ".$SISTER->get_Sister();

?>
